<p id="notice"><%= notice %></p>

<h1>Listing Distributes</h1>

<table>
  <thead>
    <tr>
      <th>Is distribute</th>
      <th>User</th>
      <th>Apply</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @distributes.each do |distribute| %>
      <tr>
        <td><%= distribute.is_distribute %></td>
        <td><%= distribute.user %></td>
        <td><%= distribute.apply %></td>
        <td><%= link_to '查看', distribute %></td>
        <td><%= link_to '修改', edit_distribute_path(distribute) %></td>
        <td><%= link_to '删除', distribute, method: :delete, data: { confirm: '你确定要删除吗？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Distribute', new_distribute_path %>
